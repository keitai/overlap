<td data-label="email">
  <%= link_to user.email, user %>
</td>

<td data-label="ratings">
  <%= link_to user.ratings.count, user %>
</td>

<td data-label="lastlogin">
	<%= user.last_sign_in_at.strftime("%m/%d/%Y") %>
</td>

<td data-label="logins">
	<%= user.sign_in_count %>	
</td>

<td data-label="edit links">
  <%= link_to "Edit user", edit_user_path(user) %>
  <%= link_to("| Delete user", user_path(user), :data => { :confirm => "Are you sure?" }, :method => :delete) unless user == current_user %>
</td>




