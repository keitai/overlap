<%= simple_form_for(@therapist) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
 		 <%= f.input :first_name %>
		 <%= f.input :last_name %>
		 <%= f.input :email %>
		 <%= f.input :location %>
		 <%= f.input :phone_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "small float-right" %>
  </div>
<% end %>
